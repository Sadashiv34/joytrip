{
  "version": 2,
  "builds": [
    { "src": "**/*.html", "use": "@now/static" },
    { "src": "**/*.js", "use": "@now/static" },
    { "src": "**/*.css", "use": "@now/static" },
    { "src": "**/*.json", "use": "@now/static" },
    { "src": "**/*.png", "use": "@now/static" },
    { "src": "**/*.jpg", "use": "@now/static" },
    { "src": "**/*.svg", "use": "@now/static" },
    { "src": "**/*.ico", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/_next/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/landing.html" }
  ]
}
